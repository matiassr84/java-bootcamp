public class Client {
	public static void main(String[] args){
		Supermarket sm=new Supermarket();
		System.out.println(sm.listsize());
		sm.AddProduct("TOMATO", 2.37);
		sm.listProducts();
		sm.AddProduct("ONION", 2.37);
		sm.listProducts();
